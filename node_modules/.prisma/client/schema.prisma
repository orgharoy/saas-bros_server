generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model employee {
    id        String   @id @unique @default(uuid())
    firstName String
    lastname  String
    email     String   @unique
    password  String
    createdAt DateTime @default(now())
}
